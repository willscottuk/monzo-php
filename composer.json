{
  "name": "willscottuk/monzo-php",
  "description": "An API client for Monzo Bank (https://monzo.com)",
  "type": "library",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Amelia Ikeda",
      "email": "amelia@dorks.io"
    },
    {
      "name": "Will Scott",
      "email": "will@cronx.co"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.1",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "ramsey/uuid": "^4.2",
    "guzzlehttp/guzzle": "^7.5"
  },
  "require-dev": {
    "phpunit/phpunit": "12.2.2",
    "laravel/socialite": "^5.4",
    "symfony/var-dumper": "^7.0",
    "php-vcr/php-vcr": "^1.3",
    "laravel/framework": "12.2.0"
  },
  "autoload": {
    "psr-4": {
      "Amelia\\Monzo\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Amelia\\Tests\\Monzo\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Amelia\\Monzo\\MonzoServiceProvider"
      ]
    }
  }
}
